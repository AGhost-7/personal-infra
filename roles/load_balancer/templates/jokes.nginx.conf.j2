
server {
	listen 80;
	server_name jokes.jonathan-boudreau.com;

	location / {
		proxy_pass http://localhost:8066;
	}
}

server {
	listen 443 ssl;

	server_name jokes.jonathan-boudreau.com;

	ssl_protocols TLSv1.3 TLSv1.2;
	ssl_ciphers ECDHE-ECDSA-AES256-GCM-SHA384:ECDHE-RSA-AES256-GCM-SHA384:ECDHE-ECDSA-CHACHA20-POLY1305:ECDHE-RSA-CHACHA20-POLY1305:ECDHE-PSK-CHACHA20-POLY1305;
	ssl_certificate /etc/letsencrypt/live/jonathan-boudreau.com/fullchain.pem;
	ssl_certificate_key /etc/letsencrypt/live/jonathan-boudreau.com/privkey.pem;

	location / {
		proxy_pass http://localhost:8066;
	}

}
